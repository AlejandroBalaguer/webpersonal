{
  "name": "paginapersonal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "frontend": "vite --host",
    "requirements": "py -m pip install -r requirements.txt",
    "backend": "py -m uvicorn src.python.ia_gatos_perros.main:app --reload --port 8000",
    "dev": "concurrently \"npm run frontend\" \"npm run requirements\" \"npm run backend\"",
    "build": "vite build --minify esbuild --outDir dist",
    "analyze": "vite build --mode analyze",
    "lint": "eslint .",
    "preview": "vite preview",
    "lighthouse": "lighthouse http://localhost:4173 --view"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bootstrap": "^5.3.5",
    "bootstrap-icons": "^1.11.3",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "react": "^19.0.0",
    "react-awesome-reveal": "^4.3.1",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.0",
    "react-tsparticles": "^2.12.2",
    "tsparticles": "^3.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  }
}
